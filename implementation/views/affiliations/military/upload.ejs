<% include ../../partials/header %>

<div class="container"> 

    <div class="upload-header">
        <h1>Military document upload</h1>
    </div>

    <div class="upload-container">

        <span>Important - the documentation must display:</span>
        <ul>
            <li>Your full name (<%= firstName %> <%= lastName %>)</li>
            <li>The name of the organization (<%= organizationName %>)</li>
            <li>Your military status (<%= affiliationType %>)</li>
            <li>Must have an issue date within the past three months.</li>
        </ul>

        <p>If this information appears incorrect, click <a href="verify">here</a> to try again.</p>

        <span>Example of acceptable documentation:</span>
        <ul>
            <li><%= exampleDocument %></li>
        </ul>

        <% include ../../partials/upload-form-submit %>
        <% include ../../partials/sheerid-logo %>
    </div>

</div>

<% include ../../partials/footer %>
